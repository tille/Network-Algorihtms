#include <iostream>

using namespace std;

const int M = 20;
int array[M];

void init(){
  for( int i = 0; i < 20; ++i ) array[i] = i;
  for( int i = 0; i < 20; ++i ) cout << array[i] << endl;
}

int main(){
  init();
  cout << "resultado final: " << endl;
  for( int i = 0; i < 20; ++i ) cout << array[i] << endl;
  return 0;
}

